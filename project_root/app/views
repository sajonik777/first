+            print("Volume Size: ", volume.child_get_string("size"))

+def print_network_interface_info(output):
+    if (output.results_errno() != 0):
+        result = output.results_reason()
+        print("Failed to get network interface info: %s" % result)
+    else:
+        print("Network Interface Information: ")
+        interfaces = output.child_get("attributes-list").children_get()
+        for interface in interfaces:
+            print("Interface Name: ", interface.child_get_string("name"))
+            print("Interface IP: ", interface.child_get_string("ip-address"))

